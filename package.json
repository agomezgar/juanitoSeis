{
  "name": "juanitoseis",
  "version": "1.0.0",
  "description": "Prueba de app para comunicación Firmata con Arduino",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w serialport",
    "build:linux": "electron-builder -l",
    "build:win": "electron-builder -w",
    "inst": "electron-builder --win --x64",
    "postinstall": "electron-builder install-app-deps",
    "package": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "github.com/agomezgar/firmata"
  },
  "keywords": [
    "firmata",
    "antonio",
    "antoniotecnocr"
  ],
  "author": "Antonio Gómez",
  "license": "ISC",
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "latest",
    "electron-packager": "latest",
    "electron-rebuild": "latest",
    "node-abi": "^3.56.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "overrides": {
    "electron-rebuild": {
      "node-abi": "3.24.0"
    }
  },
  "dependencies": {
    "johnny-five": "rwaldron/johnny-five",
    "serialport": "^8.0.8"
  }
}
